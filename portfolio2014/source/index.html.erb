</script>

<!--/////////////////////////////////////////////skills model template ////////////-->

<script type='text/template' id='skills-model-template'>
      <li class="title">{{title}}</li>
      <img src="{{featureImage}}" class="view-project">
      <li class="skillContent">{{content}}</li>
      {{if video}}
      <iframe src="{{video}}" width="500" height="281" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
      {{end}}
</script>

